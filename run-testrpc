if [ ! "$(docker ps -q -f name=testrpc)" ]; then
    echo "no testrpc found"
    if [ "$(docker ps -aq -f status=exited -f name=testrpc)" ]; then
        echo "removing exited testrpc"
        docker rm testrpc
    fi
    echo "running testrpc"
    docker run --rm -d --name testrpc -p 8545:8545 desmart/testrpc:latest -l 6761676
fi
